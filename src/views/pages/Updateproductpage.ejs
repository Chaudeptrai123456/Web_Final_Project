<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">

<!-- jQuery library -->
<script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>

<!-- Popper JS -->
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

<!-- Latest compiled JavaScript -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
    <title>Sign in page</title>
</head>
<body>
    
    <table>
        <form action="/api/admin/handleproductupdate" method="post">
        <tr>
            <td>Id Product</td>
            <td><input value="<%=product.dataValues.id%>" name="id"  readonly /></td>
        </tr>
        <tr>
            <td>Name Product</td>
            <td><input value="<%=product.dataValues.nameproduct%>" name="nameproduct"  /></td>
        </tr>

        <tr>
            <td>Quantity</td>
            <td><input value="<%=product.dataValues.quantity%>" name="quantity" type="number" /></td>
        </tr>

        <tr>
            <td>Cost</td>
            <td><input value="<%=product.dataValues.cost%>" name="cost"  /></td>
        </tr>

        <tr>
            <td>description</td>
            <td><input value="<%=product.dataValues.description%>" name="description"  /></td>
        </tr>

        <tr>
            <td>
            <input type="submit" value="update"/>

            </td>
        </tr>


    </table>
</form>
   
</body>
</html>