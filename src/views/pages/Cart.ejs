<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">

<!-- jQuery library -->
<script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>

<!-- Popper JS -->
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

<!-- Latest compiled JavaScript -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
    <title>Login Page</title>
</head>
<body>
    <h1>Total cost <%=total%>  Id user <%=iduser%></h1>
    <% for (var i = 0;i<productlist.length;i++) { %>
    
        <div class="card" style="width: 18rem;">
            
            <div style="text-align:center">Id <%=productlist[i].id%></div>
            <div class="card-body">
              <h5 class="card-title"> Name  <%=productlist[i].Name%></h5>
              <h5 class="card-title">  cost   <%=productlist[i].cost%></h5>
              <h5 class="card-title">  amount   <%=productlist[i].amount%></h5>
              <h5 class="card-title">  Date   <%=productlist[i].Date%></h5>
                
                 
              <p class="card-text">  description  <%=productlist[i].description%></p> 
            </div>
          </div>
          <br>
    <%}%>
</body>
</html>