      <h1>History of shopping</h1>
 
      
    <h1>Total cost <%=total%>  Id user <%=iduser%></h1>
    <% for (var i = 0;i<productlist.length;i++) { %>
     <div style="  margin: 0 auto;  
     width: 200px;  
     padding: 20px;
     ">
        <div class="card" style="width: 18rem">
          <div  class="card-text" style="text-align: center"> Id <%=productlist[i].id%>
          <img class="card-img-top" src="/<%=productlist[i].idproduct%>.jpeg"  width="160"height="250">

            <div class="card-body"style="text-align:left">
          
 
        
            
              <h5 class="card-text" > Name  <%=productlist[i].Name%></h5>
              <h5 class="card-text">  cost   <%=productlist[i].cost%></h5>
              <h5 class="card-text">  amount   <%=productlist[i].amount%></h5>
              <h5 class="card-text">  Date   <%=productlist[i].Date%></h5>
              <h5 class="card-text">  status   <%=productlist[i].status%></h5>
                 
              <p class="card-text">  description  <%=productlist[i].description%></p> 
            </div>
        </div>
        </div>
          <br>
        </div>
    <%}%>
